CREATE TABLE PROBLEM_TBL(
    ID NUMBER PRIMARY KEY ,
    PARENT_ID NUMBER(20),
    DESCRIPTION NVARCHAR2(100),
    DATE_TIME TIMESTAMP,
    SENDER NVARCHAR2(20) ,
    RECEIVER NVARCHAR2(20),
    ANSWER NVARCHAR2(20) ,
    STATUS NVARCHAR2(20) ,
    DELETED NUMBER (1)
);
CREATE SEQUENCE PROBLEM_SEQ START WITH 1 INCREMENT BY 1;